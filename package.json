    "@nuxtjs/tailwindcss": "^6.1.3",
    "@wevisdemo/ui": "^6.1.1",
    "axios": "^1.2.6",
    "core-js": "^3.25.3",
    "firebase": "^9.17.1",
    "nocodb-sdk": "^0.105.2",
    "nuxt": "^2.16.1",
    "papaparse": "^5.3.2",
    "vue": "^2.7.10",
    "vue-server-renderer": "^2.7.10",
    "vue-sharer": "^1.0.0",
    "vue-template-compiler": "^2.7.10",
    "vue-uuid": "^3.0.0"
