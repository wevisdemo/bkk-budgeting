    "@nuxtjs/svg": "^0.4.0",
    "@types/lodash": "^4.14.191",
    "@types/papaparse": "^5.3.7",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "husky": "^8.0.1",
    "jszip": "^3.10.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
